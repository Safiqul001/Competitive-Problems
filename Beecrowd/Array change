Link: https://judge.beecrowd.com/en/problems/view/1175

#include <bits/stdc++.h>
using namespace std;

int main(){
    int a[20], j = 0;
    for(int i = 0; i <= 19; i++){
        cin >> a[i];
    }

    for(int i = 19; i >= 0; i--){
        cout << "N[" << j << "] = " << a[i] << "\n";
        j++;
    }

    return 0;
}

Link: https://judge.beecrowd.com/en/problems/view/1160

#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        int PA, PB;
        double G1, G2;
        scanf("%d %d %lf %lf", &PA, &PB, &G1, &G2);
        int years = 0;
        while (PA <= PB) {
            PA += (int)(PA * (G1 / 100.0));
            PB += (int)(PB * (G2 / 100.0));
            years++;
            if (years > 100) {
                printf("Mais de 1 seculo.\n");
                break;
            }
        }
        if (years <= 100) {
            printf("%d anos.\n", years);
        }
    }
    return 0;
}

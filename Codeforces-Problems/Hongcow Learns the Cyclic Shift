Link: https://codeforces.com/contest/745/problem/A

#include <bits/stdc++.h>
using namespace std;

int main(){
    string st;
    cin >> st;
    set <string> s;

    int n = st.size();
    for(int i = 0; i < n; i++){
        s.insert(st);
        st = st.back() + st.substr(0, + n - 1);  //st = st.substr(1) + st[0];
    }

    cout << s.size() << endl;

    return 0;
}

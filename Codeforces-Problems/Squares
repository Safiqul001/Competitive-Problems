Link: https://codeforces.com/contest/263/problem/B

#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, k;
    cin >> n >> k;
    vector<int> a(n);
    for(int &ai : a){
        cin >> ai;
    }
    sort(a.begin(), a.end());

    for(int i = 0; i < n; ++i){
        int t = a[i];
        int cnt = n - i;
        if(cnt == k){
            cout << t << " 0\n";
            return 0;
        }
    }

    cout << "-1\n";
    return 0;
}

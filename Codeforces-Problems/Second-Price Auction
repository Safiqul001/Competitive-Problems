Link: https://codeforces.com/contest/386/problem/A

#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, ele;
    cin >> n;
    vector<pair<int,int>> vec;

    int maxi = INT_MIN;
    int max_pos = -1;

    for(int i = 1; i <= n; i++){
        cin >> ele;
        vec.push_back({ele, i});

        if(ele > maxi){
            maxi = ele;
            max_pos = i;
        }
    }

    sort(vec.begin(), vec.end(), greater<pair<int,int>>());

    vec.erase(vec.begin());

    cout << max_pos << " " << vec[0].first << "\n";

    return 0;
}

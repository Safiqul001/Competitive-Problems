Link: https://onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=1724

#include <bits/stdc++.h>
using namespace std;

int main(){
    int T, a, b;
    cin >> T;

    for(int caseNum = 1; caseNum <= T; ++caseNum){
        cin >> a >> b;
        int sum = 0;

        for(int i = a; i <= b; ++i){
            if(i % 2 == 1){
                sum += i;
            }
        }
        cout << "Case " << caseNum << ": " << sum << endl;
    }

    return 0;
}

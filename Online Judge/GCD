Link: https://onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=2412

#include <bits/stdc++.h>
using namespace std;

int gcd(int a, int b){
    while(b != 0){
        int temp = a % b;
        a = b;
        b = temp;
    }
    return a;
}

int main(){
    int N;
    while(cin >> N && N != 0){
        int G = 0;
        for(int i = 1; i < N; ++i){
            for(int j = i + 1; j <= N; ++j){
                G += gcd(i, j);
            }
        }
        cout << G << endl;
    }
    return 0;
}

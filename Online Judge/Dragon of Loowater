Link: https://onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=2267

#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, m;
    while(cin >> n >> m && (n != 0 || m != 0)){
        vector<int> heads(n);
        vector<int> knights(m);

        for(int i = 0; i < n; ++i){
            cin >> heads[i];
        }
        for(int i = 0; i < m; ++i){
            cin >> knights[i];
        }

        sort(heads.begin(), heads.end());
        sort(knights.begin(), knights.end());

        int cost = 0;
        int h = 0, k = 0;

        while(h < n && k < m){
            if(knights[k] >= heads[h]){
                cost += knights[k];
                ++h;
            }
            ++k;
        }

        if(h == n){
            cout << cost << endl;
        } else {
            cout << "Loowater is doomed!" << endl;
        }
    }
    return 0;
}

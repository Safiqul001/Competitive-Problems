Link: https://onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=2899

#include <bits/stdc++.h>
using namespace std;

int main(){
    int T;
    cin >> T;
    for(int i = 1; i <= T; ++i){
        int N;
        cin >> N;
        int maxSpeed = 0;
        for(int i = 0; i < N; ++i){
            int speed;
            cin >> speed;
            if (speed > maxSpeed){
                maxSpeed = speed;
            }
        }
        cout << "Case " << i << ": " << maxSpeed << endl;
    }
    return 0;
}

